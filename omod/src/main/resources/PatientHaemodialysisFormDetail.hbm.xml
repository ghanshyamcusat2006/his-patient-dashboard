<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.openmrs.module.patientdashboard.model">
    <class name="PatientHaemodialysisFormDetail" table="patient_haemodialysis_form_details" batch-size="500">
        <id name="id" type="int" column="id" unsaved-value="0">
            <generator class="native"></generator>
        </id>
        <property name="bp" column="bp" />
        <property name="pulse" column="pulse" />
        <property name="tmp" column="tmp" />
        <property name="vp" column="vp" />
        <property name="createdDate" column="created_date" />
        <many-to-one name="patientHaemodialysisForm" column="patient_haemodialysis_form_id" class="org.openmrs.module.patientdashboard.model.PatientHaemodialysisForm" lazy="false" foreign-key="patient_haemodialysis_form_details_fk" />
    </class>
</hibernate-mapping>